@{
    ViewData["Title"] = "New Member";
}


<form asp-controller="Member" asp-action="RegMember">
    <table class="table">
        <tr>
            <td>ID</td>
            <td><input type="text" name="id" /></td>
        </tr>
        <tr>
            <td>Title</td>
            <td><input type="text" name="title" /></td>
        </tr>
        <tr>
            <td>Author</td>
            <td><input type="text" name="author" /></td>
        </tr>
        <tr>
            <td>E-Mail</td>
            <td><input type="text" name="e_mail" /></td>
        </tr>
        <tr>
            <td>Password</td>
            <td><input type="text" name="password" /></td>
        </tr>
        <tr>
            <td>Content</td>
            <td><input type="text" name="content" /></td>
        </tr>
        <tr>
            <td><input type ="submit" value="OK" /></td>
        </tr>
        <!--submit을 통해서 입력받은 값들을 받아와 regmember 함수로 보내준d-->
    </table>
</form>